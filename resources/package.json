{
  "name": "design-to-code-workshop-resources",
  "version": "1.0.0",
  "description": "Resources and tools for the Design-to-Code Workshop",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:mcp-connection": "node scripts/test-mcp-connection.js",
    "test:azure-cv": "node scripts/test-azure-cv.js",
    "test:azure-fr": "node scripts/test-azure-fr.js",
    "test:a11y": "jest --testMatch='**/*.a11y.test.{js,ts}'",
    "generate:component": "node scripts/generate-component.js",
    "generate:batch": "node scripts/batch-generate.js",
    "generate:from-image": "node scripts/generate-from-image.js",
    "extract:tokens": "node scripts/extract-design-tokens.js",
    "analyze:design": "node scripts/analyze-design.js",
    "audit:performance": "node scripts/performance-audit.js",
    "build:lib": "rollup -c",
    "build:storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@azure/ai-form-recognizer": "^4.0.0",
    "@azure/cognitiveservices-computervision": "^8.2.0",
    "@azure/identity": "^3.3.0",
    "@azure/keyvault-secrets": "^4.7.0",
    "@azure/ms-rest-js": "^2.7.0",
    "@modelcontextprotocol/client": "^1.0.0",
    "@github/copilot-agent-sdk": "^1.0.0",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "ora": "^5.4.1",
    "redis": "^4.6.10",
    "styled-components": "^6.1.0",
    "typescript": "^5.2.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@rollup/plugin-typescript": "^11.1.5",
    "@storybook/addon-actions": "^7.5.2",
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/addon-interactions": "^7.5.2",
    "@storybook/addon-links": "^7.5.2",
    "@storybook/blocks": "^7.5.2",
    "@storybook/react": "^7.5.2",
    "@storybook/react-vite": "^7.5.2",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/fs-extra": "^11.0.3",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@types/styled-components": "^5.1.29",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rollup": "^4.3.0",
    "storybook": "^7.5.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "vite": "^4.5.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paulasilvatech/Design-to-Code-Dev.git"
  },
  "keywords": [
    "design-to-code",
    "figma",
    "ai",
    "azure",
    "component-generation",
    "workshop"
  ],
  "author": "Paula Silva",
  "license": "MIT"
}